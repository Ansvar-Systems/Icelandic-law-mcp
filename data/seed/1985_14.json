{
  "id": "14/1985",
  "type": "statute",
  "title": "Lög um framkvæmd alþjóðasamnings um gáma",
  "status": "in_force",
  "issued_date": "1985-05-10",
  "url": "https://www.althingi.is/lagas/nuna/1985014.html",
  "description": "Tóku gildi 31. maí 1985.",
  "provisions": [
    {
      "provision_ref": "1",
      "section": "1",
      "content": "Ráðherra þeim, sem fer með siglingamál, er heimilt að setja reglur til að framfylgja alþjóðasamningi, sem Ísland er aðili að, um gáma.\nRg. 624/1997.",
      "metadata": {
        "source_anchor": "G1",
        "section_format": "article"
      }
    },
    {
      "provision_ref": "2",
      "section": "2",
      "content": "Brot gegn reglum, sem settar verða samkvæmt lögum þessum, varða sektum.",
      "metadata": {
        "source_anchor": "G2",
        "section_format": "article"
      }
    },
    {
      "provision_ref": "3",
      "section": "3",
      "content": "Lög þessi öðlast þegar gildi.",
      "metadata": {
        "source_anchor": "G3",
        "section_format": "article"
      }
    }
  ]
}
